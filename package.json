	"description": "Yet another Discord Rich Presence will show information on your current activity in Visual Studio Code",
	"version": "1.0.9",
	"author": "Satoqz",
	"icon": "assets/icon.png",
	"publisher": "Satoqz",
	"repository": {
		"type": "git",
		"url": "https://github.com/Satoqz/vscode-discord.git"
	},
	"engines": {
