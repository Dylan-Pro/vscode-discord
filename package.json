{
	"name": "yet-another-discord-presence",
	"displayName": "Yet another Discord Presence",
	"description": "Yet another Discord Rich Presence will show information on your current activity in Visual Studio Code",
	"version": "1.0.5",
	"author": "Satoqz",
	"icon": "assets/icon.png",
	"publisher": "Satoqz",
	"repository": {
		"type": "git",
		"url": "https://github.com/Satoqz/vscode-discord.git"
	},
	"engines": {
		"vscode": "^1.47.0"
	},
	"categories": [
		"Visualization",
		"Other"
	],
	"extensionKind": [
		"ui",
		"workspace"
	],
	"keywords": [
		"discord",
		"presence",
		"rich presence",
		"rpc",
		"game status"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.47.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"eslint": "^6.8.0",
		"eslint-config-typescript-satoqz": "0.0.3",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.9.7",
		"vscode-test": "^1.3.0",
		"@types/discord-rpc": "^3.0.4"
	},
	"dependencies": {
		"discord-rpc": "^3.1.2"
	},
	"eslintConfig": {
		"extends": "eslint-config-typescript-satoqz"
	}
}
